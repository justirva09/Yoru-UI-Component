{
  "name": "yoru-ui",
  "version": "1.0.0",
  "description": "A Project for react ui components",
  "scripts": {
    "test": "jest",
    "test:workspace": "jest -w=",
    "test:coverage": "jest --coverage",
    "storybook": "start-storybook -p 6006",
    "build": "turbo run build",
    "build:fast": "turbo run build:fast",
    "typecheck": "tsc --noEmit",
    "clean": "pnpm -r --parallel exec rimraf dist .turbo *.log",
    "prettier": "prettier \"packages/**/*.{js,ts,tsx}\"",
    "prettier:fix": "prettier --write packages/**/*.{js,jsx,tsx,json}",
    "lint": "eslint '*/**/*.{js,ts,tsx}' --quiet --fix",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "keywords": [
    "react",
    "ui-component"
  ],
  "author": "justirva09",
  "license": "MIT",
  "dependencies": {
    "@babel/core": "^7.18.9",
    "@babel/preset-env": "^7.18.9",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@babel/runtime": "^7.18.9",
    "@storybook/addon-a11y": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-storysource": "^6.5.9",
    "@storybook/builder-webpack5": "^6.5.9",
    "@storybook/cli": "^6.5.9",
    "@storybook/manager-webpack5": "^6.5.9",
    "@storybook/react": "^6.5.9",
    "@storybook/theming": "^6.5.9",
    "@swc-node/jest": "^1.5.2",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.3.0",
    "@types/jest": "^28.1.6",
    "@types/node": "^18.0.0",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.32.0",
    "@typescript-eslint/parser": "^5.32.0",
    "button": "^1.1.1",
    "eslint": "^8.21.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "17.0.0",
    "eslint-plugin-import": "^2.25.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "lint-staged": "^13.0.3",
    "prettier": "^2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.0.0",
    "rimraf": "3.0.2",
    "ts-node": "^10.7.0",
    "tsup": "6.1.3",
    "turbo": "^1.4.2",
    "typescript": "^4.7.4"
  },
  "pnpm": {
    "overrides": {
      "react": "^18.2.0",
      "react-dom": "^18.2.0"
    }
  },
  "devDependencies": {
    "eslint-config-prettier": "^8.5.0",
    "husky": "^8.0.0"
  }
}
